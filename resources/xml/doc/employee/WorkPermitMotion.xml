<?xml version="1.0"?>
<root upper_case="false" justify="true" split_word="false" cells="false">
    <field
        name="authority_id"
        type="select"
        required="true"
        onchange="getPerson(this);"
        model="Employer:FMS"/>
    <field
        name="officer_id"
        type="select"
        required="true"
        model="Employee:Officer"/>
    <field
        name="date"
        required="true"
        type="date"
        format="dd-MM-yyyy"/>
</root>
